{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "Devknife Toolbox",
  "version": "0.1.0",
  "identifier": "hunghd.work",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://127.0.0.1:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Devknife Toolbox",
        "width": 1200,
        "height": 800,
        "minWidth": 980,
        "minHeight": 680,
        "resizable": true,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.icns",
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png"
    ]
  }
}
